{
  "name": "json-to-excel-test-case",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Convert open API json data to test case excel file with Electron + Vue + Vite.",
  "author": "Nick SUN",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "JSON",
    "Excel",
    "OpenAPI"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^23.1.2",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.0",
    "eslint": "^8.34.0",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "node-xlsx": "^0.21.0",
    "element-plus": "^2.2.33"
  }
